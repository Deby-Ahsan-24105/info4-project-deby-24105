class A:
    def metodeA(self):
        print("Ini dari class A")

    def metode1(self):
        print("ini dari kelas 1")

class B:
    def metodeB(self):
        print("Ini dari class B")

    def metode2(self):
        print("ini dari kelas 2")

class C(A, B):
    pass

obj = C()
obj.metodeA()
obj.metode1()
obj.metodeB()
obj.metode2()



